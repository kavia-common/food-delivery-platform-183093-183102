# Backend environment variables
# IMPORTANT: Do not commit actual secrets. Copy this file to .env and fill in values.

# JWT secret for token signing
SECRET_KEY=replace_me_with_a_long_random_string

# Allowed frontend origin for CORS
FRONTEND_ORIGIN=http://localhost:3000

# PostgreSQL connection string
# Example: postgresql+psycopg2://user:password@hostname:5001/fooddb
DATABASE_URL=postgresql+psycopg2://<username>:<password>@<host>:5001/<database>
